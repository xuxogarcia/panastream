{
  "name": "pana-stream",
  "version": "1.0.0",
  "description": "A simple cloud-based streaming application with S3 and CloudFront integration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "streaming",
    "video",
    "s3",
    "cloudfront",
    "mediaconvert"
  ],
  "author": "PanaStream Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3"
  }
}
